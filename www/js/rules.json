{
    "rules": {
        "chat": {
            ".read": false,
            ".write": false
        },
        "personagem": {
            ".read": false,
            ".write": false
        },
        "chats": {
            ".read": "auth != null",
            ".write": "auth != null"
        },
        "acoes": {
            ".read": "auth != null",
            ".write": "auth != null",
            ".indexOn": ["idPersonagem", "respondido"]
        },
        "acoesCtrl": {
            ".read": "auth != null",
            ".write": "auth != null"
        },
        "citacoes": {
            ".read": "auth != null",
            ".write": "auth != null"
        },
        "clanlist": {
            ".read": "auth != null",
            ".write": false
        },
        "seita": {
            ".read": "auth != null",
            ".write": false
        },
        "xps": {
            ".read": "auth != null",
            ".write": "auth != null"
        },
        "personagens": {
            ".indexOn": [
                "userid"
            ],
            ".read": "auth != null",
            ".write": "auth != null"
        },
        "personagensInfo": {
            ".read": "auth != null",
            ".write": "auth != null"
        }
    }
}